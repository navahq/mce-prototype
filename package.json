{
  "name": "mce-prototype",
  "description": "Medicare Cost Estimator prototype",
  "version": "1.0.0",
  "author": "Nava PBC <npm@navahq.com>",
  "repository": "navahq/mce-prototype",
  "dependencies": {
    "@cmsgov/design-system-core": "^1.9.0",
    "@cmsgov/design-system-layout": "^1.9.0",
    "classnames": "^2.2.5",
    "gatsby": "^1.9.157",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-plugin-sass": "^1.0.15",
    "gatsby-source-filesystem": "^1.5.11",
    "gatsby-transformer-json": "^1.0.14",
    "prop-types": "^15.6.0",
    "react-helmet": "^5.2.0"
  },
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "lint": "eslint src ./ --ext .js --ext .jsx -f table && stylelint src/**/*.scss",
    "precommit": "lint-staged",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "test": "jest"
  },
  "devDependencies": {
    "@cmsgov/eslint-config-design-system": "^1.2.0",
    "@cmsgov/stylelint-config-design-system": "^1.1.0",
    "eslint": "^4.15.0",
    "eslint-config-nava": "^2.1.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "jest": "^22.1.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.10.2",
    "stylelint": "^8.4.0",
    "stylelint-config-prettier": "^2.0.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint --fix",
      "git add"
    ]
  }
}
